{
  "httpPort": 3000,
  "allowLocalFilesAccess": true,
  "store": {
    "provider": "fs"
  },
  "templatingEngines": {
    "strategy": "dedicated-process"
  },
  "extensions": {
    "authentication" : {
      "cookieSession": {
        "secret": "<your strong secret>"
      },
      "admin": {
        "username" : "admin",
        "password": "password"
      },
      "enabled": false
    },
    "express": {
      "inputRequestLimit": "128mb"
    },
    "electron": {
      "strategy":"dedicated-process",
      "timeout": 30000
    },
    "phantom": {
      "numberOfWorkers" : 2,
      "timeout": 8000,
      "strategy": "dedicated-process"
    },
    "sample-template": {
      "createSamples": true
    }
  }
}
